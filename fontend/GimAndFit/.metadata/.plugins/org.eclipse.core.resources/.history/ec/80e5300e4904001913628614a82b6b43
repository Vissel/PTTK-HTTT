package dao;

import java.util.ArrayList;

import model.Product;

public class Product_DAO {
	
	ArrayList<Product> list;

	public Product_DAO() {
		
	}

	public ArrayList<Product> getList() {
		list.removeAll(list);
		
		
		
		
		return list;
	}

	public void setList(ArrayList<Product> list) {
		this.list = list;
	}
	
}
